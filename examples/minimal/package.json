{
  "name": "minimal",
  "version": "1.0.1-beta.2",
  "description": "A minimal Lattice App",
  "main": "index.js",
  "private": true,
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/docs/"
    ]
  },
  "scripts": {
    "test": "jest --no-cache",
    "start": "webpack-dev-server --mode=development --open",
    "prodQA": "webpack --mode=production",
    "prod": " npm run test && webpack --mode=production",
    "lint": "eslint --cache --ignore-path .npmignore .",
    "clean": "rimraf dist/**",
    "watch": "webpack --mode development --watch & nodemon dist/main"
  },
  "dependencies": {
    "@latticejs/mui-recharts": "^1.0.1-beta.2",
    "@latticejs/widgets": "^1.0.1-beta.2",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "jest-cli": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "recharts": "^1.8.5",
    "typeface-roboto": "^0.0.54"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "compression-webpack-plugin": "^1.1.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "style-loader": "^0.22.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
