{
  "env": {
  "browser": true,
  "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
  "Atomics": "readonly",
  "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
  "ecmaFeatures": {
  "jsx": true
  },
  "ecmaVersion": 2018,
  "sourceType": "module"
  },
  "plugins": [
  "react"
  ],
  "rules": {
    "react-app/jsx-a11y/href-no-hash": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true,
        "bracketSpacing": true,
        "printWidth": 120,
        "endOfLine":"auto"
      }
    ],
    "no-console": "off",
    "require-atomic-updates": "off"
  }
}